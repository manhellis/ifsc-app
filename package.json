{
  "name": "ifsc-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx concurrently \"cd client && bun run dev\" \"cd server && bun run dev\"",
    "dev:client": "cd client && bun run dev",
    "dev:server": "cd server && bun run dev",
    "build": "cd client && bun run build",
    "install:all": "bun install && cd client && bun install && cd ../server && bun install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "axios": "^1.8.4",
    "lucide-react": "^0.488.0"
  }
}
